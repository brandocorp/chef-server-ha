# Generated by Chef for <%= node[:fqdn] %>

resource <%= @resource %> {
  device    <%= @device %>;
  disk      <%= @disk %>;
  meta-disk internal;
  on <%= @primary[:fqdn] %> {
    address   <%= @primary[:ipaddress] %>:<%= @port %>;
  }
  on <%= @secondary[:fqdn] %> {
    address   <%= @secondary[:ipaddress] %>:<%= @port %>;
  }
}
